<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="@color/tw__composer_white"
    tools:context=".employer.EmployerTimeSheetActivity"
    tools:layout_editor_absoluteY="25dp">

    <Button
        android:id="@+id/button8"
        android:layout_width="175dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Project"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/time_sheet_app_bar" />

    <Button
        android:id="@+id/button7"
        android:layout_width="77dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Hrs"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/time_sheet_app_bar" />

    <TextView
        android:id="@+id/text_view_tues"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/text_view_wed"
        app:layout_constraintEnd_toStartOf="@+id/text_view_tues_proj"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/text_view_mon" />

    <TextView
        android:id="@+id/text_view_wed"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/text_view_tues" />

    <TextView
        android:id="@+id/text_view_thurs"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/text_view_wed" />

    <TextView
        android:id="@+id/text_view_fri"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/text_view_thurs" />

    <Button
        android:id="@+id/button5"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Fri"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button_attach"
        android:layout_width="74dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:textColor="@color/tw__composer_white"
        app:layout_constraintEnd_toEndOf="@+id/textview_ts_decline"
        app:layout_constraintTop_toBottomOf="@+id/textview_ts_decline"
        app:srcCompat="@drawable/baseline_attachment_black_24dp" />

    <include
        android:id="@+id/time_sheet_app_bar"
        layout="@layout/app_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="55dp"></include>


    <Button
        android:id="@+id/button_ts_accept"
        android:layout_width="343dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/round_button"
        android:text="@string/accept"
        android:textColor="@color/tw__composer_white"
        app:layout_constraintEnd_toEndOf="@+id/text_view_ts_comments"
        app:layout_constraintStart_toStartOf="@+id/text_view_ts_comments"
        app:layout_constraintTop_toBottomOf="@+id/text_view_ts_comments" />

    <Button
        android:id="@+id/textview_ts_decline"
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:background="@drawable/round_button"
        android:text="@string/decline"
        android:textColor="@color/tw__composer_white"
        app:layout_constraintEnd_toEndOf="@+id/button_ts_accept"
        app:layout_constraintStart_toStartOf="@+id/button_ts_accept"
        app:layout_constraintTop_toBottomOf="@+id/button_ts_accept" />

    <TextView
        android:id="@+id/text_view_ts_comments"
        android:layout_width="346dp"
        android:layout_height="49dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/shape"
        android:scrollbars="vertical"
        android:textSize="8sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/text_view_fri_proj"
        app:layout_constraintStart_toStartOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/text_view_fri_proj" />

    <Button
        android:id="@+id/button"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Mon"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button2"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Tues"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <Button
        android:id="@+id/button3"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Wed"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button4"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Thurs"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <TextView
        android:id="@+id/text_view_mon"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <TextView
        android:id="@+id/text_view_proj_mon"
        android:layout_width="175dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/button8"
        app:layout_constraintStart_toEndOf="@+id/text_view_mon"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <TextView
        android:id="@+id/text_view_fri_proj"
        android:layout_width="175dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_view_thurs_proj"
        app:layout_constraintStart_toEndOf="@+id/text_view_fri"
        app:layout_constraintTop_toBottomOf="@+id/text_view_thurs_proj" />

    <TextView
        android:id="@+id/text_view_tues_proj"
        android:layout_width="175dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/text_view_tues"
        app:layout_constraintTop_toBottomOf="@+id/text_view_proj_mon" />

    <TextView
        android:id="@+id/text_view_wed_proj"
        android:layout_width="175dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_view_tues_proj"
        app:layout_constraintStart_toEndOf="@+id/text_view_wed"
        app:layout_constraintTop_toBottomOf="@+id/text_view_tues_proj" />

    <TextView
        android:id="@+id/text_view_thurs_proj"
        android:layout_width="175dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_view_wed_proj"
        app:layout_constraintStart_toEndOf="@+id/text_view_thurs"
        app:layout_constraintTop_toBottomOf="@+id/text_view_wed_proj" />

    <Button
        android:id="@+id/button6"
        android:layout_width="77dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@color/tw__composer_blue"
        android:enabled="false"
        android:text="Day"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/time_sheet_app_bar" />


</android.support.constraint.ConstraintLayout>