<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".timesheet.HistoryCSV">

    <TextView
        android:id="@+id/text_view_total_hs_csv"
        android:layout_width="382dp"
        android:layout_height="327dp"
        android:layout_marginTop="72dp"
        android:background="@drawable/shape"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Total" />

    <Button
        android:id="@+id/button_print_time_sheet"
        android:layout_width="382dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="8dp"
        android:background="@drawable/round_button"
        android:text="PRINT"
        android:textColor="@color/tw__composer_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_view_total_hs_csv" />
</android.support.constraint.ConstraintLayout>