        android:theme="@style/AppTheme">
        <activity
            android:name=".EmployerProfile"
            android:icon="@drawable/timesheet"
            android:label="ETime">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/register_title" />
        <activity
            android:name=".CreateProfile"
            android:label="@string/register_title" />
        <activity android:name="com.twitter.sdk.android.core.identity.OAuthActivity" />
        <activity android:name=".EmployeeProfile" />
        <!-- <activity android:name=".EmployerProfile"></activity> -->
        <activity android:name=".CreateProject"></activity>
